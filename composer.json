{
    "name": "hotwired-laravel/hotreload",
    "description": "Hot reloading for Hotwire",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HotwiredLaravel\\Hotreload\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tony Messias",
            "email": "tonysm@hey.com"
        }
    ],
    "require-dev": {
        "tightenco/duster": "^3.1"
    },
    "require": {
        "illuminate/support": "^11.36",
        "illuminate/http": "^11.36"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\HotwiredLaravel\\Hotreload\\HotreloadServiceProvider"
            ]
        }
    }
}
